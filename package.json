    "@material-ui/icons": "^4.11.2",
    "app": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.5.1",
    "config": "^3.3.6",
    "convert-csv-to-json": "^1.3.3",
    "create": "^0.0.2",
    "date-fns": "^2.28.0",
    "express": "^4.17.2",
    "float": "^1.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.6",
    "mongoose-currency": "^0.2.0",
    "mongoose-float": "^1.0.4",
    "react": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-uuid": "^1.0.2"
